<UserControl x:Class="HCI___Assignment_Prototype.Page.Homepage.Homepage_BeforeLogin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:HCI___Assignment_Prototype.Page.Homepage">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0, 10, 50, 0">
                <TextBox Width="200" Margin="0, 0, 50, 0">Search Movies......</TextBox>
                <Button Width="100" Margin="0, 0, 10, 0" Click="SignUpButto_OnClick">SIGN UP</Button>
                <Button Width="100" Style="{StaticResource MaterialDesignRaisedAccentButton}"
                        Click="LogInButton_OnClick">
                    LOG IN
                </Button>
            </StackPanel>
            <local:Homepage_BottomPart></local:Homepage_BottomPart>
        </StackPanel>
    </Grid>
</UserControl>